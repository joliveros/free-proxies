{
  "name": "free-proxies",
  "version": "0.1.1",
  "main": "dist/Proxies.js",
  "repository": "git+ssh://git@github.com:joliveros/free-proxies.git",
  "dependencies": {
    "babel": "^5.5.6",
    "cheerio": "^0.19.0",
    "co": "^4.5.4",
    "co-phantom": "https://github.com/kolodny/co-phantom.git",
    "co-redis": "^1.2.1",
    "debug": "^2.2.0",
    "generator-foreach": "^0.1.0",
    "lodash": "^3.9.3",
    "ms": "^0.7.1",
    "qs": "^3.1.0",
    "redis": "^0.12.1",
    "superagent": "^1.2.0",
    "superagent-proxy": "^0.3.2",
    "validate-ip": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^2.1.1",
    "co-mocha": "^1.1.0",
    "jshint": "^2.6.3",
    "jshint-stylish": "^1.0.1",
    "mocha": "^2.2.1",
    "should": "^5.2.0"
  },
  "engines": {
    "node": ">=0.10.37"
  },
  "scripts": {
    "build": "babel src -d dist",
    "dev": "NODE_ENV=development node --debug --harmony src/server.js",
    "install": "npm run build",
    "local": "NODE_ENV=local nodemon --debug --harmony src/server.js",
    "start": "NODE_ENV=production node --harmony src/server.js",
    "test": "npm run test-mocha",
    "test-jshint": "jshint -c .jshintrc src/**/*.js --exclude src/**/*.spec.js --reporter node_modules/jshint-stylish/stylish.js",
    "test-mocha": "NODE_ENV=test mocha --require co-mocha --harmony-generators ./src/**/*spec.js --compilers js:babel/register",
    "test-watch": "NODE_ENV=test DEBUG=proxies,test mocha --watch --require co-mocha --harmony-generators ./src/**/*spec.js --compilers js:babel/register",
    "test-watch-debug": "NODE_ENV=test DEBUG=proxies,test mocha -d --watch --require co-mocha --harmony-generators ./src/**/*spec.js --compilers js:babel/register"
  }
}
